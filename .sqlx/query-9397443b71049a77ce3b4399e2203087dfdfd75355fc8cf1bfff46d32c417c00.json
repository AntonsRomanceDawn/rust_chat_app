  "query": "\n            SELECT m.* FROM user_messages m\n            JOIN room_members rm ON m.room_id = rm.room_id\n            WHERE m.room_id = $1\n            AND rm.user_id = $2\n            AND m.created_at >= rm.joined_at\n            ORDER BY m.created_at DESC\n            LIMIT $3 OFFSET $4\n            ",
