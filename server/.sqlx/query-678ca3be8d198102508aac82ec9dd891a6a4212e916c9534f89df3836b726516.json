  "query": "\n            SELECT r.id, r.name, r.creator_id, r.admin_id, r.created_at\n            FROM rooms r\n            JOIN room_members rm ON r.id = rm.room_id\n            WHERE rm.user_id = $1\n            ORDER BY r.created_at DESC\n            ",
